<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckMenuItem?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16" fx:controller="fxWebProtocols.WebProtocolsGUIController">
   <bottom>
      <Label fx:id="statusBar" text="Hello World!" BorderPane.alignment="CENTER" />
   </bottom>
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
            <Menu mnemonicParsing="false" text="File">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#handleClose" text="Close" />
              </items>
            </Menu>
          <Menu mnemonicParsing="false" text="Clients" />
          <Menu mnemonicParsing="false" text="Servers">
            <items>
                  <CheckMenuItem mnemonicParsing="false" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
